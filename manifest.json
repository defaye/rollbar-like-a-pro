{
  "manifest_version": 3,
  "name": "Rollbar like a Pro",
  "version": "1.0",
  "permissions": [
    "https://app.rollbar.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.rollbar.com/*"],
      "js": [
        "jquery-3.7.1.min.js",
        "content.js"
      ],
      "css": ["styles.css"]
    }
  ]
}
